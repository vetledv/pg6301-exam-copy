{
  "compilerOptions": {
    "target": "es6",
     "jsx": "react-jsx",
    "module": "commonjs",
    "typeRoots": ["node_modules/@types"], 
    "allowJs": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
